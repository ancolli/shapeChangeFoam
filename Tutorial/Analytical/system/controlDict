FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     currentDistributionMovingMesh;

startFrom       startTime;//latestTime;//

startTime       0;

stopAt          endTime;

endTime         25000;//13610;//19050;//

deltaT          1;

writeControl    runTime;

writeInterval   2500;//1361;//1905;//

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


