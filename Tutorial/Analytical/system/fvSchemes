FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default        Euler;
}

gradSchemes
{
    default         leastSquares;//none;
}

divSchemes
{
    default         none;
}

laplacianSchemes
{
    default         none;Gauss linear corrected;//
    laplacian(diffusivity,cellMotionU) Gauss linear orthogonal;// 
    laplacian(keff,fi) Gauss linear orthogonal; //
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    fi               ;
}


